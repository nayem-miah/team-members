    "groups",
    "team",
    "teams",
    "group",
    "grid",
    "members"
  ],
  "description": "A team member grid.",
  "supports": {
    "html": false,
    "align": [
      "full",
      "wide"
    ]
  },
  "textdomain": "team-members",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js",
